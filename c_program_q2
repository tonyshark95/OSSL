#include<iostream>
using namespace std;


void printarray(int arr[],int size)
{
    for(int i=0; i<size; i++)
    {
        cout<<arr[i]<<" ";
    }
}
int main()
{
    int size;
    cout<<"enter the size of the array:"<<endl;
    cin>>size;
    int arr[100];
    cout<<"enter the elements of the array:"<<endl;
    for(int i=0 ;i<size ;i++)
    {
        cin>>arr[i];
    }
    printarray(arr,size);
}
